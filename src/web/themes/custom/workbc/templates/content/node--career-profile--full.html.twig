<div class="career-profile-header">

  <div class="career-profile-header-breadcrumb">
    {{ drupal_block('system_breadcrumb_block', wrapper=false) }}
  </div>

  {% if viewmodel['hero']['image'] or viewmodel['hero']['description'] %}
    <div class="career-profile-header-banner hero-banner hero-banner-profile">

      <div class="hero-image-wrapper">
        <div class="hero-image-overlay"></div>
        {{ viewmodel['hero']['image'] }}
      </div>

      <div class="hero-content-container container-lg">
        <div class="hero-content-wrapper">
          <div class="hero-title">
            <h1>{{ viewmodel['hero']['title'] }}</h1>
          </div>

          <div class="hero-noc">(NOC {{ viewmodel['hero']['noc'] }})</div>

          {% if viewmodel['hero']['hoo'] %}
            <div class="hero-hoo">
              <img src="/{{ directory }}/assets/images/hoo-icon.svg" alt="{{ 'High opportunity occupation'|t }}" />
              <span>{{ 'High opportunity occupation'|t }}</span>
            </div>
          {% endif %}

          <div class="hero-content">
            {{ viewmodel['hero']['description'] }}
          </div>
        </div>
      </div>

    </div>
  {% endif %}

  <div class="career-profile-header-social container-lg">
    {{ drupal_block('social_sharing_buttons_block', wrapper=false) }}
  </div>
</div>

<article{{ attributes.addClass(classes) }}>

  <div class="career-statistics-wrapper">
    <div class="container-lg profile-information-card-deck">

      {% for statistic in viewmodel['top_statistics'] %}

        <div class="profile-information-card {{ statistic['icon_class'] }}">
          <div class="profile-information-card-icon">
          </div>
          <div class="profile-information-card-content">
            <div class="profile-information-card-title">
              {{ statistic['title'] }}
            </div>
            <div class="profile-information-card-value">
              {{ statistic['value'] }}
              {# {{ statistic['value']|length > 30 ? statistic['value']|slice(0, 30)|split(' ')|slice(0, -1)|join(' ') ~ ' â€¦' : statistic['value'] }} #}
            </div>
          </div>
          {% if statistic['source'] %}
            <div class="profile-information-card-infotip">
              <a tabindex="0" class="btn btn-link info-tooltip" role="button" data-bs-toggle="popover" data-bs-container="body" data-bs-trigger="click hover" data-bs-placement="bottom" data-bs-custom-class="workbc-popover" title="hidden title" data-bs-html="true" data-bs-content="<em>Source:</em> {{ statistic['source'] }}"></a>
            </div>
          {% endif %}
        </div>

      {% endfor %}

    </div>
  </div>

  <div class="career-details-wrapper">
    <div class="career-content-tabs ">
      <div class="container-lg scroll-container">
        <ul class="nav nav-tabs career-content-tab-list scroll" id="career-content-tabs" role="tablist">
          {% for key, tab in viewmodel['tab_sections'] %}
            <li class="nav-item" role="presentation">
              <a class="nav-link career-content-tab-button{{ loop.first ? ' active'}}" id="{{key}}-tab" data-bs-toggle="tab" data-bs-target="#{{key}}-content" aria-controls="{{key}}-content" aria-selected="true" role="tab" href="#">{{tab['title']}}</a>
            </li>
          {% endfor %}
        </ul>
      </div>
      <div class="container-lg scroll-affordance-shim"></div>
    </div>

    <div class="container-lg">
      <div class="career-content-container">
        <div class="tab-content career-content-main">

          <div class="tab-pane active" id="career_overview-content" role="tabpanel" aria-labelledby="career_overview-tab">
            <h2 class="introduction">{{ viewmodel['tab_sections']['career_overview']['title'] |t }}</h2>
            <div>{{ viewmodel['tab_sections']['career_overview']['description'] |raw }}</div>
            
            <div class="career-content-section">
              <h2>Duties</h2>
              <div>{{ viewmodel['tab_sections']['career_overview']['duties_description'] |raw }}</div>
            </div>

            <div class="career-content-section">
              <h2>Salary</h2>
              <div>{{ viewmodel['tab_sections']['career_overview']['salary_description'] |raw }}</div>

              <div class="card-profile">
                <div class="card-profile__content-small">
                  <h3 class="card-profile__content-header">Annual Salary<a tabindex="0" class="btn btn-link info-tooltip card-profile__content-tooltip" role="button" data-bs-toggle="popover" data-bs-container="body" data-bs-trigger="click hover" data-bs-placement="bottom" data-bs-custom-class="workbc-popover" title="hidden title" data-bs-html="true" data-bs-content="{{ viewmodel['tab_sections']['career_overview']['salary_annual_salary_note']|raw }}"></a></h3>
                  <span class="card-profile__content-value">{{ viewmodel['tab_sections']['career_overview']['salary_annual_salary'] |raw }}</span>
                </div>
                <div class="card-profile__content-divider"></div>
                <div class="card-profile__content-small">
                  <h3 class="card-profile__content-header">Provincial Hourly Rate</h3>
                  <span class="card-profile__content-table">{{ viewmodel['tab_sections']['career_overview']['salary_provincial_hourly_rate_markup'] |raw }}</span>
                </div>  
                <div class="card-profile__source">
                  <em>Source:</em> {{ viewmodel['tab_sections']['career_overview']['salary_source'] |raw }}
                </div>
              </div>
            </div>

            <div class="career-content-section">
              <h2>Work Environment</h2>
              <div class="card-profile">
                <div class="card-profile__content-small">
                  <h3 class="card-profile__content-header"># Workers Employed</h3>
                  <span class="card-profile__content-value">{{ viewmodel['tab_sections']['career_overview']['work_environment_workers_employed'] |raw }}</span>
                </div>
                <div class="card-profile__content-divider"></div>
                <div class="card-profile__content-small">
                  <h3 class="card-profile__content-header">% Employed Full Time</h3>
                  <span class="card-profile__content-value">{{ viewmodel['tab_sections']['career_overview']['work_environment_percent_fulltime'] |raw }}</span>
                </div>
                <div class="card-profile__source">
                  <em>Source:</em> {{ viewmodel['tab_sections']['career_overview']['work_environment_source'] |raw }}
                </div>
              </div>
              {{ viewmodel['tab_sections']['career_overview']['work_environment_description'] |raw }}
            </div>

            <div class="career-content-section">
              <h2>Career Pathways</h2>
              {{ viewmodel['tab_sections']['career_overview']['career_pathways_description'] |raw }}
            </div>

            <div class="career-content-section">
              <h2>Related Careers</h2>
              {{ viewmodel['tab_sections']['career_overview']['related_careers'] }}
            </div>

            <div class="career-content-section">
              <h2>Occupational Interests</h2>
              {{ viewmodel['tab_sections']['career_overview']['occupational_interests_intro'] |raw }}
              <div class="card-occupational-interest-deck">
                {% for interest in viewmodel['tab_sections']['career_overview']['occupational_interests_items'] %}
                  {{ interest }} 
                {% endfor %}
              </div>
            </div>

            <div class="career-content-section">
              <h2>Job Titles</h2>

              <ul class="career-content-job-titles">
                {% for jobtitle in viewmodel['tab_sections']['career_overview']['job_titles'] %}
                  <li>{{ jobtitle.value }}</li>
                {% endfor %}  
              </ul>
            </div>

          </div>

          <div class="tab-pane" id="education-content" role="tabpanel" aria-labelledby="education-tab">
            <h2 class="introduction">{{ viewmodel['tab_sections']['education']['title'] |t }}</h2>
            <div>{{ viewmodel['tab_sections']['education']['description'] |raw }}</div>

            <div class="career-content-section">
              <h2>Education programs in B.C.</h2>
              <div>{{ viewmodel['tab_sections']['education']['education_programs_introduction'] |raw }}</div>

              {% for program in viewmodel['tab_sections']['education']['education_programs_list'] %}
                {{ program.0['#markup']|raw }}
              {% endfor %}
            </div>


             <div class="career-content-section">
              <h2>Skills</h2>
              <div>{{ viewmodel['tab_sections']['education']['skills_introduction'] |raw }}</div>

              <div data-static-load-more-container data-static-load-more-initial="3" data-static-load-more-step="7">
                <div data-static-load-more-items>
                  {% for skill in viewmodel['tab_sections']['education']['skills_list'] %}
                    {{ skill|raw }}
                  {% endfor %}
                </div>
                <div class="static-load-more-button-container">
                  <button data-static-load-more-trigger class="btn btn-secondary">Show More</button>
                </div>
              </div>
            </div>
          </div>

          <div class="tab-pane" id="labour_market-content" role="tabpanel" aria-labelledby="labour_market-tab">
            <h2 class="introduction">{{ viewmodel['tab_sections']['labour_market']['title'] |t }}</h2>
            <div>{{ viewmodel['tab_sections']['labour_market']['description'] |raw }}</div>


            <h2>{{ viewmodel['tab_sections']['labour_market']['employment_title'] }}</h2>
            <div>{{ viewmodel['tab_sections']['labour_market']['employment_introduction'] }}</div>
            
            <div class="card-profile">
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Employment</h3>
                <span class="card-profile__content-value">{{ viewmodel['tab_sections']['labour_market']['employment_number']|raw }}</span>
              </div>
              <div class="card-profile__source">
                <em>Source:</em> {{ viewmodel['tab_sections']['labour_market']['employment_number_source']|raw }}
              </div>
            </div>

            <div class="card-profile">
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Employment by Region</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['labour_market']['employment_by_region']|raw }}</span>
              </div>
              <div class="card-profile__source">
                <em>Source:</em> {{ viewmodel['tab_sections']['labour_market']['employment_by_region_source']|raw }}
              </div>
            </div>

            <h2>{{ viewmodel['tab_sections']['labour_market']['outlook_title'] }}</h2>
            <div>{{ viewmodel['tab_sections']['labour_market']['outlook_introduction']|raw }}</div>

            <div class="card-profile">
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Expected Job Openings (2019 - 2029)</h3>
                <span class="card-profile__content-value">{{ viewmodel['tab_sections']['labour_market']['outlook_openings_expected']|raw }}</span>
              </div>
              <div class="card-profile__source">
                <em>Source:</em> {{ viewmodel['tab_sections']['labour_market']['outlook_openings_expected_source']|raw }}
              </div>
            </div>

            <div class="card-profile">
              <div class="card-profile__content-small">
                <h3 class="card-profile__content-header">Forecasted Job Openings</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['labour_market']['outlook_openings_forecast']|raw }}</span>
              </div>
              <div class="card-profile__content-divider"></div>
              <div class="card-profile__content-small">
                <h3 class="card-profile__content-header">Forecasted Employment Growth Rate</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['labour_market']['outlook_openings_forecast_growth_rate']|raw }}</span>
              </div>
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Composition of Job Openings</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['labour_market']['outlook_openings_composition']|raw }}</span>
              </div>
              <div class="card-profile__source">
                <em>Source:</em> {{ viewmodel['tab_sections']['labour_market']['outlook_openings_forecast_source']|raw }}
              </div>
            </div>

            <div class="card-profile">
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Job Openings by Region (2019 - 2029)</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['labour_market']['outlook_openings_by_region']|raw }}</span>
              </div>
              <div class="card-profile__source">
                <em>Source:</em> {{ viewmodel['tab_sections']['labour_market']['outlook_openings_by_region_source']|raw }}
              </div>
            </div>

          </div>

          <div class="tab-pane" id="industry_highlights-content" role="tabpanel" aria-labelledby="industry_highlights-tab">
            <h2 class="introduction">{{ viewmodel['tab_sections']['industry_highlights']['title'] |t }}</h2>
            <div>{{ viewmodel['tab_sections']['industry_highlights']['description'] |raw }}</div>

            <div class="card-profile">
              <div class="card-profile__content">
                <h3 class="card-profile__content-header">Forecasted Job Openings by Industry</h3>
                <span class="card-profile__content-item">{{ viewmodel['tab_sections']['industry_highlights']['job_openings'] |raw }}</span>
              </div>
