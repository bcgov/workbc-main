<?php

/**
 * @file
 * WorkBC theme file.
 */

/**
 * Implements hook_preprocess_page().
 */
function workbc_preprocess_page(&$variables) {
  $variables['#attached']['drupalSettings']['responsive_menu']['custom'] = [
    'options' => [
      'navbar' => [],
      'navbars' => [
        [
          'position' => 'top',
          'use' => TRUE,
          'content' => [
            'title',
            'close'
          ]
        ]
      ]
    ],
  ];
}
